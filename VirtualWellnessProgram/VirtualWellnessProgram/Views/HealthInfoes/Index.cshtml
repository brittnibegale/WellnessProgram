@model IEnumerable<VirtualWellnessProgram.Models.HealthInfo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UniqueCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Height)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Weight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Smoker)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BodyFatAmt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hdl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ldl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CholesterolTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Triglycerides)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UniqueCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Height)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Weight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Smoker)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BodyFatAmt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hdl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ldl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CholesterolTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Triglycerides)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
